<h2>Updating a Pitch</h2>

<form action="/pitches/<%= @pitch.id %>" method="post">
  <input id="hidden" type="hidden" name="_method" value="patch">
  Title: <input type="text" name="title" value="<%= @pitch.title %>">
  Industry: <input type="text" name="industry" value="<%= @pitch.industry %>">
  Funding: <input type="float" name="funding" value="<%= @pitch.funding %>">
  <input type="submit" value="submit">
</form>